/* pages/jobIntention/jobIntention.wxss */
.mt20 {
  margin-top: 20rpx;
}

.t-name .input, .t-address .value {
  text-align: right;
  overflow: hidden;
  color: #b2b2b2;
  height: 44rpx;
}

.add-page {
  display: flex;
  flex-direction: column;
  font-size: 32rpx;
  color: #676767;
  padding: 30rpx 0;
}

.mod-a {
  padding: 0 20rpx;
  border: 1px solid #f0f0f0;
  background-color: #fff;
}

.mod {
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  align-items: center;
  height: 90rpx;
  padding: 0 10rpx;
}

.arrow-r {
  width: 30rpx;
  height: 30rpx;
  margin-left: 10rpx;
}

.long-btn{
  width: 92%;
  margin-top: 20rpx;
  background-color:#4cabaf;
  color:#fff;
}